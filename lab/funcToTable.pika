::funcToTable = function {
	args(@count, @minX, @maxX, @func);
	s = '';
	for (i = 0; i < count; ++i) {
		x = minX + (i / (count - 1)) * (maxX - minX);
		y = func(x);
		s #= x # TAB # y # LF;
	};
	s
};


root		<-	(c=ws
				/ c=qString
				/ c=aString
				/ c=symbol						{ $$[$c] = $c }
				/ .
				)* !.

qString		<-	'"' ( !["\\\0-\37] . / escape )* '"'

aString		<-	"'" ( !['\\\0-\37] . / escape )* "'"

escape		<-	"\\" ( 'x' hex hex / 'u' hex hex hex hex / . )

hex			<-	[0-9a-fA-F]

symbol		<-	[a-zA-Z_][$a-zA-Z_0-9]*

ws			<- 	([ \t\r\n] / comment)+

comment		<-	'/*' (!'*/' .)* '*/'
				/ "//" (!eol .)* (eol / eof)

eol			<-	"\r\n" / "\n" / "\r"

eof			<-	!.

